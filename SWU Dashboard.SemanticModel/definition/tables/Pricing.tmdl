	partition Pricing = m
		mode: import
		queryGroup: 'TCGPlayer Pricing'
		source =
				let
				    Source =
				        Table.NestedJoin(
				            Products,
				            {"productId", "group"},
				            Prices,
				            {"productId", "group"},
				            "Prices",
				            JoinKind.LeftOuter
				        ),
				    ExpandedPrices =
				        Table.ExpandTableColumn(
				            Source,
				            "Prices",
				            {
				                "lowPrice",
				                "midPrice",
				                "highPrice",
				                "marketPrice",
				                "directLowPrice",
				                "subTypeName"
				            },
				            {
				                "lowPrice",
				                "midPrice",
				                "highPrice",
				                "marketPrice",
				                "directLowPrice",
				                "subTypeName"
				            }
				        ),
				    ReorderedColumns =
				        Table.ReorderColumns(
				            ExpandedPrices,
				            {
				                "group",
				                "productId",
				                "name",
				                "cleanName",
				                "imageUrl",
				                "categoryId",
				                "groupId",
				                "url",
				                "modifiedOn",
				                "imageCount",
				                "cardNumber",
				                "abbreviation",
				                "setName",
				                "subTypeName",
				                "lowPrice",
				                "midPrice",
				                "highPrice",
				                "marketPrice",
				                "directLowPrice"
				            }
				        ),
				    AddedPartnerURL =
				        Table.AddColumn(
				            ReorderedColumns,
				            "partnerURL",
				            each
				                "https://partner.tcgplayer.com/c/6152233/1780961/21018?u="
				                & Uri.EscapeDataString([url])
				        ),
				    TypedColumns =
				        Table.TransformColumnTypes(
				            AddedPartnerURL,
				            {
				                {"lowPrice", type number},
				                {"midPrice", type number},
				                {"highPrice", type number},
				                {"marketPrice", type number},
				                {"directLowPrice", type number},
				                {"name", type text}
				            }
				        )
				in
				    TypedColumns

	annotation PBI_NavigationStepName = Navigation

	annotation PBI_ResultType = Table

